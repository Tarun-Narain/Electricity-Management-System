socket.on('updateSetTime', function(data){
for(var i=0;i<data.length;i++)
{
  var x=data[i];
  document.getElementById(x.id+"FROM").innerHTML=x.from;
  document.getElementById(x.id+"TO").innerHTML=x.to;
}
});
function setNotify(e)
{
e = e || window.event;
   e = e.target || e.srcElement;
    console.log(e.id);
var from =document.getElementById(e.id+"NOTIN").value;
if(isNaN(parseFloat(from))||parseFloat(from)<=0)
alert('Invalid Format');
else{
socket.emit('setNotify', {id: e.id, notify:from});
alert('Limit Set');
}
}

function setTime(e)
{
e = e || window.event;
   e = e.target || e.srcElement;
    console.log(e.id);
var from =document.getElementById(e.id+"FROM").value;
var to =document.getElementById(e.id+"TO").value;
if(from.length>5||to.length>5){
alert('Invalid Format');
}
if(from.split(':').length<2||to.split(':').length<2){
alert('Invalid Format');
}
else{
socket.emit('settime', {id: e.id, from : from, to:to});
alert('Timing Set  from : '+from+" TO : " +to);
}
}

