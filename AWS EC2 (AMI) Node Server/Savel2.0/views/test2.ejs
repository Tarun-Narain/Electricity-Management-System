  <script>
	
	
 var socket = io();
 var working=1;
socket.emit('rpiq', {status:'?'});
socket.emit('getThirty', {});
socket.on('trainedData', function(data){
console.log(data);
document.getElementById('predicted').innerHTML="Rs. "+data[data.length-1].substr(0,5); 
});
var notificationText;
document.getElementById('ass').addEventListener('click', function(){
console.log("added" + document.getElementById('name').value);
var name = document.getElementById('name').value;
var power = document.getElementById('power').value;
var gpio = document.getElementById('gpio').value;
if(name.length<=1)
alert('Name Must Be greater Than 1');
else if(isNaN(parseFloat(power))||parseFloat(power)<=0)
alert('Wrong Power Format');
else if(gpio.split('-').length<2)
alert('Wrong GPIO Format')
else{
socket.emit('addDevice', {name: name,power: power,gpio: gpio.toUpperCase()});
location.reload();}
});

socket.on('rpic', function(data){
working=1;
document.getElementById('rpistatus').innerHTML = "<b><small>"+data.status+"</small></b>";
document.getElementById('rpistatus').style.color = "green";
notificationText="Raspberry Pi Connected";
 demo.showGreenNotification('top', 'right'); 
for(var i=0;i<data.switches.length;i++)
{
var x=document.getElementById(data.switches[i].id+"STATUS");
if(data.switches[i].status=='on'){
document.getElementById(data.switches[i].id).src="/assets/a2.jpg";
	document.getElementById(data.switches[i].id+"STATUS").innerHTML = "ON";
	}
if(data.switches[i].status=='off'){
document.getElementById(data.switches[i].id).src="/assets/a1.jpg";
	document.getElementById(data.switches[i].id+"STATUS").innerHTML = "OFF";
	}
}


});
socket.on('toogle', function(data){
console.log('caught toogle');
if(data.status=='on'){
document.getElementById(data.id).src="/assets/a2.jpg";
	document.getElementById(data.id+"STATUS").innerHTML = "ON";
	}
	
if(data.status=='off'){
document.getElementById(data.id).src="/assets/a1.jpg";
	document.getElementById(data.id+"STATUS").innerHTML = "OFF";
	}
});


socket.on('update', function(data){

for(var i=0;i<data.length;i++)
{
  
  var x=data[i];
  if(x.status=='on'){
document.getElementById(x.id).src="/assets/a2.jpg";
	document.getElementById(x.id+"STATUS").innerHTML = "ON";
	}
	
  document.getElementById(x.id+"CTE").innerHTML="&nbsp &nbsp &nbsp &nbsp<b> Time Elapsed :</b> " +x.CTE;
  document.getElementById(x.id+"TTE").innerHTML="&nbsp &nbsp &nbsp &nbsp <b>Total Time Elapsed :</b> "+x.TTE;
var str=(((+x.TTE/3600)* (+x.power/1000))*5).toString();
  document.getElementById(x.id+"COST").innerHTML="&nbsp &nbsp &nbsp &nbsp <b>Total Cost :</b> "+ str.substr(0, 7);

if(x.status=='off'){
document.getElementById(x.id).src="/assets/a1.jpg";
	document.getElementById(x.id+"STATUS").innerHTML = "OFF";
	}
  
}
});
